#include<stdio.h> 
//#include 

void main() {

int arr[3][3];
int i,j;

//clrscr();

printf("3x3 Matrix : ");


//Read the elements of the 3x3 matrix
for(i=0;i<=2;i++) {
    
    for(j=0;j<=2;j++) {
        
        printf("\nEnter element at location %d,%d : ",i+1,j+1);
        scanf("%d",&arr[i][j]);
        
         
    }

}
 printf("\n\n 3x3 matrix : \n\n");


//Read the elements of the 3x3 matrix
for(i=0;i<=2;i++) {
    
    for(j=0;j<=2;j++) {
        
        printf("%d  ",arr[i][j]);
         
    }
    
    printf("\n");

}



//getch();  
   
}

